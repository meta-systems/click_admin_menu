<div class="adminMenuP">
    <a href="/click_admin_menu/clear_cache" title="Clear Cache" class="adminMenuCC"></a>
    <a href="#" class="adminMenuOpen" title="Admin menu"></a>

    <div class="adminMenu">
        <div class="adminMenuTitle">{{title}}</div>
        {% for item in items %}
            <a href="{{item.link}}"{% if target %} target="_blank"{% endif %}>{{item.label}}</a>
        {% endfor %}
        <a href="#" class="click_admin_menu_add_btn">Add a page</a>
        {% for shortcut in shortcuts %}
            <a href="{{shortcut.url}}"{% if target %} target="_blank"{% endif %}>{{shortcut.title}}</a>
        {% endfor %}
    </div>
</div>